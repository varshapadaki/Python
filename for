for i in range(1,6):
    for j in range(1,i+1):
        print('*',end=' ')
    print()
    
for i in range(1,6):
    for j in range(1,i+1):
        print(i,end=' ')
    print()
    
for i in range(97,123):
    for j in range(97,i+1):
        print(chr(i),end=' ')
    print()

for i in range(97,123):
    for j in range(97,i+1):
        print(chr(j),end=' ')
    print()
